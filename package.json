{
  "name": "zxmf-react-cli",
  "version": "1.0.0",
  "description": "The react dev client server app cli",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack --config build/webpack.config.client.js",
    "build:server": "webpack --config build/webpack.config.server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "dev:server": "cross-env NODE_ENV=development nodemon server/server.js",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "start": "cross-env NODE_ENV=production node server/server.js",
    "eslint": "eslint $(git diff-index --cached HEAD --name-only --diff-filter '.js$')",
    "pre-commit": "eslint",
    "precommit.silent": "true",
    "lint": "eslint .",
    "fix": "eslint --fix ."
  },
  "author": "zhangximufeng",
  "license": "ISC",
  "pre-commit": [
    "fix",
    "lint"
  ],
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.18.3",
    "circular-json": "^0.5.5",
    "ejs": "^2.6.1",
    "ejs-compiled-loader": "^1.1.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mobx": "^5.1.0",
    "mobx-react": "^5.2.5",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "react": "^16.4.2",
    "react-async-bootstrapper": "^2.1.1",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "serialize-javascript": "^1.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.1.0",
    "hot-module-replacement": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "memory-fs": "^0.4.1",
    "pre-commit": "^1.2.2",
    "react-hot-loader": "^4.0.0",
    "rimraf": "^2.6.2",
    "serve-favicon": "^2.5.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  }
}
